<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <RelativeLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:id="@+id/frameLayout1">
    <LinearLayout android:id="@+id/linearLayoutBack2" 
android:layout_height="match_parent" android:layout_width="match_parent" android:orientation="vertical"
    android:paddingLeft="6dp"
  android:paddingRight="6dp" android:background="#d9dce0">
    <TextView android:background="#94b734" 
    android:gravity="center_vertical|left" android:id="@+id/textView1" 
    	android:text="MARCH" android:layout_width="fill_parent" android:layout_height="90dp"
    	android:paddingLeft="6dp"
    	android:textColor="@android:color/white" 
    	android:textStyle="bold"
    	></TextView>
	  <ListView android:id="@+id/listView1" 
	    	android:layout_height="400dp" 
	    	android:layout_width="match_parent" 
	    	android:textStyle="bold" android:fadingEdge="none"
	    	></ListView>
    
    
    <include layout="@layout/buttonbar"/>	
  	  </LinearLayout>
  	  <include layout="@layout/dropdownlayout" android:id="@+id/dropDLayout" />
    
  	  </RelativeLayout>
</LinearLayout>
